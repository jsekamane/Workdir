@article{Marciukaitis2010,
	Abstract = {Planuojama, kad 2010 m. v{\.e}jo elektrini{\k u} park{\k u} bendra {\k \i}rengtoji galia Lietuvoje pasieks 200 MW. Did\v{z}iausi{\k a} dal{\k \i} v{\.e}jo elektrini{\k u} park{\k u} numatoma {\k \i}rengti Lietuvos paj{\=u}ryje, kur v{\.e}jo i\v{s}tekliai yra did\v{z}iausi. Iki \v{s}iol v{\.e}jo energijos i\v{s}tekliai Lietuvoje buvo vertinami pagal atliktus pavienius v{\.e}jo grei\v{c}io matavimus bei meteorologijos sto\v{c}i{\k u} duomenis. Lietuvoje spar\v{c}iai populiar{\.e}jant v{\.e}jo energetikai, reikalingi sistemingi fundamentiniai v{\.e}jo energijos i\v{s}tekli{\k u} kaitos tyrimai, ypa\v{c} paj{\=u}rio regione, kur numatyta {\k \i}rengti daugiausia v{\.e}jo elektrini{\k u} park{\k u}. \v{S}iame darbe atlikta v{\.e}jo grei\v{c}io ir krypties matavimo 2001--2002 m. duomen{\k u} Giruliuose, prie Kretingos, Vilky\v{c}i{\k u} ir Taurag{\.e}s statistin{\.e} analiz{\.e}, i\v{s}analizuoti \v{s}i{\k u} parametr{\k u} kitimo d{\.e}sningumai bei nustatytas v{\.e}jo grei\v{c}io pasiskirstymas Lietuvos paj{\=u}rio regione. Nustatyta, kad {\k \i}vairiose Lietuvos paj{\=u}rio regio- no vietov{\.e}se v{\.e}jo j{\.e}gaini{\k u} v{\.e}jara\v{c}io auk\v{s}tyje v{\.e}jingumo s{\k a}lygos yra pana\v{s}ios, o v{\.e}jo parametr{\k u} kitimo d{\.e}sningumus labiausiai lemia atstumas nuo j{\=u}ros ir paj{\=u}rio topografija. D{\.e}l pakrant{\.e}s topografijos poveikio vidutinis nuo j{\=u}ros pu\v{c}ian\v{c}io v{\.e}jo greitis 100 m auk\v{s}tyje 20 km atstumu nuo pakrant{\.e}s suma\v{z}{\.e}ja apie 22 \%, lyginant su v{\.e}jo grei\v{c}iu ties j{\=u}ros pakrante. Rakta\v{z}od\v{z}iai: atsinaujinantieji energijos i\v{s}tekliai, v{\.e}jo energija, v{\.e}jo j{\.e}gain{\.e}s},
	Author = {Mar\v{c}iukaitis, Mantas and Erlickyt{\.e}-Mar\v{c}iukaitien{\.e}, Regina and Tumosa, Algis},
	File = {:Users/jonassekamane/Dropbox/Economics/Courses/E12/BA2/Articles/Lithuania/marciukaitis\_erlickyte-marciukaitiene\_tumosa.pdf:pdf},
	Journal = {Energetika},
	Number = {1},
	Pages = {27--34},
	Title = {{V{\.e}jo grei\v{c}io kitimo d{\.e}sningum{\k u} Lietuvos paj{\=u}rio regione tyrimas}},
	Volume = {55},
	Year = {2009}}

@article{Marciukaitis2008,
	Abstract = {Wind power is not widely used in Lithuania yet. Currently only few small-scale wind turbines with total capacity of 0.995MW are operating. However, Lithuania is planning to install wind power plants with total capacity of 200MW by the year 2010. In this paper, wind energy resource assessment experience as well as current situation and future prospective of wind energy usage in Lithuania are reviewed. Main features of Lithuanian electrical system are presented and issues related to wind energy integration in electrical networks are discussed. The aim of this work is to point up the importance of wind power prediction in Lithuania. Thus a short review of wind power prediction methods is presented which has shown that selection of most suitable prediction models for Lithuanian conditions requires detailed research. r 2006 Elsevier Ltd. All rights reserved.},
