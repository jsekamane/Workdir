@unpublished{Grogg,
	Abstract = {Alternative energy sources have become much more necessary as fossil fuels are depleted and pollute the environment. Wind energy is one of the most cost effective of all types of renewable energy. It does not create pollution or waste and the fuel, wind, is not used faster than it is produced. However, to make wind a viable source of energy---electricity in particular--- careful design of wind-capturing machines is necessary. A variety of principles of physics are used to create wind turbines that can efficiently capture energy from the wind. This paper discusses the wind and how the parts of a wind turbine---blades, rotor, gears, generator, and electronics---operate to capture wind energy and turn it into electricity. Focus is given to horizontal axis wind turbines (HAWT), the most common and efficient type of wind energy conversion device.},
	Author = {Grogg, Kira},
	File = {:Users/jonassekamane/Dropbox/Economics/Courses/E12/BA2/Articles/Grogg.pdf:pdf},
	Pages = {1--41},
	Title = {{Harvesting the Wind : The Physics of Wind Turbines}},
	Year = {2005}}

